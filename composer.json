  "type": "drupal-module",
  "require": {
    "cweagans/composer-patches": "*",
    "drupal/bootstrap_styles": ">=1.1.3 || ~1.2.1",
    "drupal/bootstrap_layout_builder": ">=2.1.2",
    "drupal/entity_clone": "^2.0@alpha || ^2",
    "drupal/layout_builder_blocks": "*",
